{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "json-format",
  "title": "Format JSON",
  "description": "Formats a JSON file with a selected indentation.",
  "icon": "command-icon.png",
  "author": "takuyahara",
  "categories": [
    "Data",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "minify-json",
      "title": "Minify JSON",
      "description": "Minify a JSON object",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3"
  },
  "devDependencies": {
    "@types/node": "~18.13.0",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.4",
    "react-devtools": "^4.27.2",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "ray build -e dist -o dist"
  }
}